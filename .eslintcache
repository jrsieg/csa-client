pe='submit'\n                              label=\"Continue\"  \n                            \n                            >Continue</FormButton>\n\n                            <FormButtonSecondary type='submit'\n                              label=\"Back\"\n                              onClick={this.back}\n  \n                            \n                            >Back</FormButtonSecondary>\n\n                        </Form>\n                    </FormContent>\n                </FormWrap>\n            </Container>\n        </>\n  \n        )\n    }\n}\n\nconst styles = {\n    button: {\n        margin: 15\n    }\n}\n\nexport default FormProduceDetails\n","/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/CSASignUp/Success.js",[],"/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/SignUpLanding/SignupLanding.js",["190","191","192","193","194","195","196"],"import React from 'react';\nimport { \n  Container,\n  FormWrap,\n  Icon,\n  FormContent,\n  Form,\n  FormH1,\n  FormLabel,\n  FormInput,\n  FormButton,\n  FormButtonSecondary,\n  FormLink,\n  Text,\n  BtnLink,\n  BtnLinkSecondary\n} from './SignUpLandingElements';\nimport { NavBtnLink } from '../../Navbar/NavbarElements';\n\n\n\n\n\nfunction SignupLanding() {\n\n  return (\n    <>\n    <Container>\n        <FormWrap>\n            <Icon to=\"/\">myCSA</Icon>\n            <FormContent>\n                <Form>\n                    <FormH1>Sign in as a...</FormH1>\n\n\n                    <BtnLink\n                      label=\"Continue\"\n                      to=\"/customersignup\"\n\n                    \n                    >Customer</BtnLink>\n\n                    <BtnLinkSecondary\n                        label=\"Back\"\n                        to=\"/csasignup\"\n\n\n                      \n                      >CSA</BtnLinkSecondary>\n\n                </Form>\n            </F