                   <div>\n                        <div>\n                        <ServicesContainer id=\"services\">\n                            <ServicesH1>Browse CSAs</ServicesH1>\n                                <ServicesWrapper>\n                                \n                                    {this.state.profile.map((info) =>\n                                    \n                                        <ServicesCard>\n                                            <ServicesIcon src={Icon1}/>\n                                                <ServicesH2>{info.farmName}</ServicesH2>\n                                                <ServicesH2>{info.email}</ServicesH2>\n                                                <ServicesP>{info.farmName} sell carrots, beef, and chicken.</ServicesP>\n                                                (sessionToken ?\n                                                <BtnWrap>\n                                                    <Button onClick = {() => this.handleSubmit(info.id)}\n                                                    \n                                                    >Join this CSA</Button> \n                                                </BtnWrap>: null)\n                                        </ServicesCard>\n                                    )}\n\n                             </ServicesWrapper>\n                            </ServicesContainer>\n\n                        </div>\n\n                    </div>\n                )}\n            </div>\n        )\n    }\n\n\n\n}\n\n","/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/Profiler/ProfilerElements.js",[],"/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/csaAuth/csaSignup.js",["156"],"import React, {useState} from 'react';\nimport { \n    Container,\n    FormWrap,\n    Icon,\n    FormContent,\n    Form,\n    FormH1,\n    FormLabel,\n    FormInput,\n    FormButton,\n    Text,\n    FormLink\n} from './CSAAuthElements';\n\n\nconst CSASignup = (props) => {\n    const [farmName, setFarmName] = useState('');\n    const [firstName, setFirstName] = useState('');\n    const [lastName, setLastName] = useState('');\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n\n    let handleSubmit = (event) => {\n        event.preventDefault();\n        fetch(\"http://localhost:3000/csa/signup\", {\n            method: 'POST',\n            body: JSON.stringify({farmName: farmName, firstName: firstName, lastName: lastName, email: email, password: password}),\n            headers: new Headers({\n                'Content-Type': 'application/json'\n            })\n        }).then(\n            (response) => response.json()\n        ).then((data) => {\n            props.updateToken(data.sessionToken)\n        })\n    }\n\n    return(\n        <>\n        <Container>\n            <FormWrap>\n                <Icon to=\"/\">myCSA</Icon>\n                <FormContent>\n                    <Form onSubmit={handleSubmit}>\n                        <FormH1>Create a new account</FormH1>\n\n                        <FormLabel htmlFor='for'>Farm Name</FormLabel>\n                        <FormInput type='Farm Name' onChange={(e) => setFirstName(e.target.value)} name=\"farmName\" value={farmName}/>\n\n\n                        <FormLabel htmlFor='for'>First Name</FormLabel>\n                        <FormInput type='First Name' onChange={(e) => setFirstName(e.target.value)} name=\"firstName\" value={firstName}/>\n\n                        <FormLabel htmlFor='for'>Last Name</FormLabel>\n                        <FormInput type='Last Name' onChange={(e) => setLastName(e.target.value)} name=\"lastName\" value={lastName}/>\n\n                        <FormLabel htmlFor='for'>Email</FormLabel>\n                        <FormInput type='Email' onChange={(e) => setEmail(e.target.value)} name=\"email\" value={email} required/>\n\n                        <FormLabel htmlFor='for'>Password</FormLabel>\n                        <FormInput type='Password' onChange={(e) => setPassword(e.target.value)} name=\"password\" value={password} required />\n\n                        <FormButton type='submit'>Continue</FormButton>\n                        <FormLink to='/csalogin'>Sign in</FormLink>\n                        <Text>Forgot password</Text>\n                    </Form>\n                </FormContent>\n            </FormWrap>\n        </Container>\n        </>\n\n    )\n}\n\nexport default CSASignup;","/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/csaAuth/csaLogin.js",[],"/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/userAuth/userSignup.js",[],"/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/userAuth/userAuthElements.js",[],"/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/userAuth/userLogin.js",[],"/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/csaAuth/CSAAuthElements.js",[],"/Users/Jonathan/Desktop/ElevenFiftyProjects/Javascript August 2020/redBadge/CSA APP/csa-client/src/components/auth/LoginModal/LoginModal.js",["157","158"],"\nimport React, {useRef, useEffect, useCallback, useState} from 'react';\nimport {useSpring, animated} from 'react-spring';\nimport { Background, ModalWrapper, ModalImg, ModalContent, CloseModalButton, Form, FormH1, FormLabel, FormInput, FormButton, Text } from './LoginModalElements';\n\n\nconst LoginModal = ({showModal, setShowModal}) => {\n\n    \n    const modalRef = useRef();\n\n    const animation = useSpring ({\n        config: {\n            duration: 250\n        },\n        opacity: showModal 